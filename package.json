{
  "name": "sav",
  "version": "0.0.1",
  "description": "sav core library",
  "main": "dist/sav.cjs.js",
  "module": "dist/sav.es.js",
  "jsnext:main": "dist/sav.es.js",
  "scripts": {
    "start": "watch -p \"+(test|src)/*.js\" -c \"npm run lint -- --fix && npm test\"",
    "build": "rollup -c",
    "lint": "standard rollup.config.js test/** src/**",
    "na": "nyc ava ./test",
    "pretest": "npm run lint",
    "test": "npm run na",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/savjs/sav.git"
  },
  "author": {
    "name": "jetiny",
    "email": "86287344@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/savjs/sav/issues"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "chai": "^3.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "nyc": "^10.0.0",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.7.1",
    "standard": "^8.5.0",
    "watch-cli": "^0.2.1"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "test"
    ]
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "stage-3"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-es2015-modules-commonjs"
    ]
  },
  "homepage": "https://github.com/savjs/sav#readme"
}